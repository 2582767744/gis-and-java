<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jdrx.platform.sample.dao.AccountDAO">

    <!-- 根据编号获得帐号 -->
    <select id="getById" resultType="AccountPO">
        SELECT
       *
        FROM message a
        WHERE
        a.del_flag = 0 AND
          a.id = #{id}
    </select>

</mapper>